<form id="cfg-form">
  <p>
    <input id="cfg-host-field" type="text" placeholder="Host" required="" value="{{host}}">
    <button type="reset">Clear</button>
  </p>
  <p>
    <input id="cfg-port-field" type="number" required="" value="{{port}}">
    <button type="reset">Clear</button> 
  </p>
  <p>
    <a id="cfg-connect-btn" class="bb-button recommend" href="#">Connect</a> 
  </p>
</form>
